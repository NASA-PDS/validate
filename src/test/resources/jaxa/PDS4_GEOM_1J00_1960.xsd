     <xs:minInclusive value="0"/>
     <xs:maxInclusive value="360"/>
	   </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="minimum_subspacecraft_longitude">
    <xs:annotation>
      <xs:documentation>The minimum_subspacecraft_longitude attribute
